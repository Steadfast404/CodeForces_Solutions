#include<bits/stdc++.h>
#include<string.h>
#define ll long long int
#define br <<"\n";
#define yes "YES"
#define no "NO"
#define For(i,n) for(int i=0;i<n;i++)
#define Forran(i,n,a) for(int i=n;i<a;i++)
#define Forrev(i,n) for(int i = n-1; i >= 0; i--)
#define fastio ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int main()
{
    int t,n,i=0,flag;
    string str;
    cin>>t;
    while(t--)
    {
        cin>>n;
        cin>>str;
        flag=0;
        For(i,n)
        {
            if(str[i]=='0'){
                flag=1;
                break;
            }
        }
        if(flag){
            cout<< yes br;
        }
        else{
            cout<< no br;
        }
    }
    return 0;
}
